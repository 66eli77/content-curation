	<input type="checkbox" id="clipboard_file_{{cid}}" name="clipboard_list_files"></input>
	<label for="clipboard_file_{{cid}}" class="clipboard_item">
		{{#if folder}}<div class="pull-left tog_folder"><span class="glyphicon glyphicon-menu-up"></span></div>{{/if}}
		<span class="glyphicon glyphicon-{{file_icon}}"></span>
		{{file.title}}
		<span class="pull-right delete_content glyphicon glyphicon-trash" aria-hidden="true"></span>
		<span class="pull-right edit_content glyphicon glyphicon-pencil" aria-hidden="true"></span>
	</label>
{{#if folder}}<ul id="clipboard_item_{{cid}}_sub" class="list-unstyled subdirectory"></ul>{{/if}}